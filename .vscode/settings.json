// Settings in this file to overwrite default and user settings.
{
  // Use prettier to format javascript and JSON files
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.words": [
    "AABB",
    "bezier",
    "brotli",
    "Catmull",
    "Cineon",
    "clearcoat",
    "codepaths",
    "componentwise",
    "Cubemap",
    "cubemaps",
    "cubeuvmaps",
    "denormalize",
    "depthbuffer",
    "Drawbuffers",
    "Equirectangular",
    "Framebuffers",
    "genericity",
    "glsl",
    "gltf",
    "Gouraud",
    "interpolant",
    "interpolants",
    "keyframes",
    "lerp",
    "lightmap",
    "linewidth",
    "matcap",
    "mipmaps",
    "multisampled",
    "Phong",
    "pingpong",
    "Premultipled",
    "Reinhard",
    "renderlayer",
    "slerp",
    "smootherstep",
    "subclip",
    "swapchain",
    "tonemapping",
    "Toon",
    "treeshake",
    "Voronoi",
    "webxr",
    "wireframe"
  ],
  "cSpell.ignoreWords": [
    "Christer",
    "Elsevier",
    "Kaufmann"
  ]
}
