// Settings in this file to overwrite default and user settings.
{
  "files.trimTrailingWhitespace": true,
  "files.exclude": {
    ".git": true,
    "**/.DS_Store": true
  },
  "search.exclude": {
    "**/node_modules": true
  },
  "files.insertFinalNewline": true,
  "[plaintext]": {
    "files.insertFinalNewline": false
  },
  "[javascript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "files.trimTrailingWhitespace": false
  },
  "cSpell.words": [
    "AABB",
    "bezier",
    "brotli",
    "Catmull",
    "Cineon",
    "clearcoat",
    "codepaths",
    "componentwise",
    "Cubemap",
    "cubemaps",
    "cubeuvmaps",
    "denormalize",
    "depthbuffer",
    "Drawbuffers",
    "Equirectangular",
    "execa",
    "Framebuffers",
    "genericity",
    "glsl",
    "gltf",
    "Gouraud",
    "gzipped",
    "interpolant",
    "interpolants",
    "keyframes",
    "lerp",
    "lightmap",
    "linewidth",
    "matcap",
    "mipmaps",
    "multisampled",
    "Phong",
    "pingpong",
    "Premultipled",
    "Raycaster",
    "Reinhard",
    "renderlayer",
    "slerp",
    "smootherstep",
    "subclip",
    "swapchain",
    "tonemapping",
    "Toon",
    "treeshake",
    "Voronoi",
    "webdriverio",
    "webxr",
    "wireframe"
  ],
  "cSpell.ignoreWords": ["Christer", "Elsevier", "Kaufmann"]
}
